{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://witchhuntserver.onrender.com/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://witchhuntserver.onrender.com/:path*/"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
